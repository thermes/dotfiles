"*":
  "atom-fuzzy-grep":
    detectGitProjectAndUseGitGrep: false
    grepCommandString: "/usr/local/bin/ag -i --nocolor --nogroup --column"
  core:
    themes: [
      "one-dark-ui"
      "solarized-dark-syntax"
    ]
  editor:
    autoIndentOnPaste: false
    fontFamily: "Ricty"
    fontSize: 15
    invisibles: {}
    lineHeight: 1
    showIndentGuide: true
    softWrap: true
  "exception-reporting":
    userId: "fe833149-6963-6819-adbc-2b6b19c64306"
  "git-diff":
    showIconsInEditorGutter: true
  "highlight-selected":
    lightTheme: true
  "linter-rubocop":
    command: "/Users/thermes/.anyenv/envs/rbenv/versions/2.3.0/bin/rubocop"
  minimap:
    plugins:
      "find-and-replace": true
      "find-and-replaceDecorationsZIndex": 0
      "git-diff": true
      "git-diffDecorationsZIndex": 0
      "highlight-selected": true
      "highlight-selectedDecorationsZIndex": 0
      "minimap-autohide": true
      "minimap-autohideDecorationsZIndex": 0
  welcome:
    showOnStartup: false
